          <span class="kd">let</span> <span class="nx">vals</span> <span class="o">=</span> <span class="nx">stringifyJSON</span><span class="p">(</span><span class="nx">obj</span><span class="p">[</span><span class="nx">key</span><span class="p">]);</span>
